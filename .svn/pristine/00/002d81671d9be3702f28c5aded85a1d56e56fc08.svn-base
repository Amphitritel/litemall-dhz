import request from '@/utils/request'

export function listAddress(data) {
  return request({
    url: '/userAddress/findUserAddressList',
    method: 'post',
    data
  })
}

export function createAddress(data) {
  return request({
    url: '/userAddress/create',
    method: 'post',
    data
  })
}

export function readAddress(data) {
  return request({
    url: '/userAddress/read',
    method: 'get',
    data
  })
}

export function updateAddress(data) {
  return request({
    url: '/userAddress/update',
    method: 'post',
    data
  })
}

export function deleteAddress(data) {
  return request({
    url: '/userAddress/delete',
    method: 'post',
    data
  })
}
