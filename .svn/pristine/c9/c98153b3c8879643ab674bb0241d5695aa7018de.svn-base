import request from '@/utils/request'

export function listGoodsClassify(data) {
  return request({
    url: '/category/queryPageByMap',
    method: 'post',
    data
  })
}


export function createGoodsClassify(data) {
  return request({
    url: '/category/insert',
    method: 'post',
    data
  })
}

export function updateGoodsClassify(data) {
  return request({
    url: '/category/update',
    method: 'post',
    data
  })
}
